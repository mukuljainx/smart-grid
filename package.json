{
  "name": "@crafts/smart-grid",
  "version": "0.0.3",
  "private": false,
  "main": "dist/cjs/index.js",
  "typings": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server",
    "doc": "webpack --config webpack.config.js",
    "build": "rm -rf dist && npm run build:cjs && npm run build:esnext && npm run css",
    "css": "npm run css:compile && npm run css:prefix && npm run css:end",
    "css:compile": "sass src/Grid:dist/cjs && sass src/Grid:dist/esm",
    "css:prefix": "postcss dist/cjs/**/*.css dist/esm/**/*.css --use autoprefixer --replace",
    "css:end": "replace-in-file .scss .css dist/**/*.js",
    "build:cjs": "tsc -m commonjs --outDir dist/cjs -p tsconfig.build.json",
    "build:esnext": "tsc -m esnext --outDir dist/esm -p tsconfig.build.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "autoprefixer": "^9.7.4",
    "css-loader": "^3.4.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "lodash-es": "^4.17.15",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "replace-in-file": "^5.0.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "fast-memoize": "^2.5.1"
  }
}
